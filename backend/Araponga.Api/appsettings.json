{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Postgres": "Host=localhost;Port=5432;Database=araponga;Username=araponga;Password=araponga"
  },
  "Persistence": {
    "Provider": "InMemory",
    "ApplyMigrations": false
  },
  "PresencePolicy": {
    "Policy": "ResidentOnly"
  },
  "Jwt": {
    "Issuer": "Araponga",
    "Audience": "Araponga",
    "SigningKey": "dev-only-change-me",
    "ExpirationMinutes": 60
  },
  "AllowedHosts": "*"
}
